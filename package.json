{
  "name": "typescriptexpressserver",
  "version": "1.0.0",
  "description": "npm init -y\r tsc --init",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:watch\"",
    "start:server": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "start:watch": "tsc --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/nodemon": "^1.19.2",
    "concurrently": "^8.0.1",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
